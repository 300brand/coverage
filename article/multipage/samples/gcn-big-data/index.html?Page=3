<script>
$(function() {

    var sticky_navigation_offset_top = $('#navWrapper').offset().top;
    var sticky_navigation = function(){
        var scroll_top = $(window).scrollTop(); // our current vertical position from the top

        if (scroll_top > sticky_navigation_offset_top) {
            $('#navWrapper').css({ 'position': 'fixed', 'top':0, 'height': 50});
                        $('#contentWrapper2col').css({ 'margin-top': 50}); 
            $('.nav_stretch').css({ 'display': 'block', 'visibility':'visible'});
            
        } else {
            $('#navWrapper').css({ 'position': 'relative',  'background': '#fff', 'height': 50, 'z-index': '10000'});
                        $('#contentWrapper2col').css({ 'margin-top': 0,'top':0}); 
            $('.nav_stretch').css({ 'display': 'none', 'visibility':'hidden'});
        }
    };

    // run function on load
    sticky_navigation();

    // run it every time you scroll
    $(window).scroll(function() {
         sticky_navigation();
    });


});
</script>





    
    
    
    
